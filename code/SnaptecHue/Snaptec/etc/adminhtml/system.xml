<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <section id="snaptec" translate="label" sortOrder="999" showInDefault="1" showInWebsite="1" showInStore="1">
            <label>Snaptec</label>
            <tab>general</tab>
            <resource>Magento_Backend::content</resource>
            <group id="snaptec_group" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
                <label>Snaptec Setting</label>
                <field id="disable_output" translate="label" type="select" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Disable Output</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                    <comment>Enable or Disable</comment>
                </field>
            </group>
        </section>
    </system>
</config>